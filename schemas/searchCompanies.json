{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "http://example.com/example.json",
	"type": "object",
	"examples": [
		{
			"name": "book",
			"minEmployees": 100,
			"maxEmployees": 450
		}
	],
	"required": [],
	"properties": {
		"name": {
			"$id": "#/properties/name",
			"type": "string",
			"description": "Name of the company to search database.",
			"default": "",
			"examples": [ "book" ]
		},
		"minEmployees": {
			"$id": "#/properties/minEmployees",
			"type": "integer",
			"description": "Minimum number of employees for a company.",
			"default": "",
			"minimum": 0,
			"examples": [ 100 ]
		},
		"maxEmployees": {
			"$id": "#/properties/maxEmployees",
			"type": "integer",
			"description": "Maximum number of employees for a company to be considered.",
			"default": "",
			"minimum": 0,
			"examples": [ 450 ]
		}
	},
	"additionalProperties": false
}
